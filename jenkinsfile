node{
    cleanWs()
    try{
        def buildNum = env.BUILD_NUMBER 
        def branchName= env.BRANCH_NAME
    
        print buildNum
        print branchName

        stage('Atelier - clone '){
        git "https://github.com/Dialsyp/Atelier.git"
        }

        stage('PremiereEtape'){
            sh "echo 'hello world'"
        }
         stage('deuxiemeEtape'){
            sh"echo 'hello world'"
        }
    }
    finally{
        cleanWs()
    }
}